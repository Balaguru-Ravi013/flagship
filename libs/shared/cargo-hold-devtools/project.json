{
  "root": "libs/shared/cargo-hold-devtools",
  "sourceRoot": "libs/shared/cargo-hold-devtools/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/cargo-hold-devtools",
        "main": "libs/shared/cargo-hold-devtools/src/index.ts",
        "tsConfig": "libs/shared/cargo-hold-devtools/tsconfig.lib.json",
        "assets": ["libs/shared/cargo-hold-devtools/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/cargo-hold-devtools/**/*.{ts,js}"],
        "hasTypeAwareRules": true
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/cargo-hold-devtools"],
      "options": {
        "jestConfig": "libs/shared/cargo-hold-devtools/jest.config.js",
        "passWithNoTests": true
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": ["open-source"]
}
